{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "MurFFy Docs",
  "css": [
    "styles.css"
  ],
  "colors": {
    "primary": "#8b5cf6",
    "light": "#8b5cf6",
    "dark": "#0a0a0c"
